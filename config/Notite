CREATE TABLE employees (
    id int AUTO_INCREMENT,
    lastname varchar(255),
    firstname varchar(255),
    job varchar(255),
    PRIMARY KEY (ID)
);

insert into employees(lastname, firstname, job) values ("bla", "DAni","servitor");
insert into employees(lastname, firstname, job) values ("Vasile", "Vecinu","Alcoolist");

